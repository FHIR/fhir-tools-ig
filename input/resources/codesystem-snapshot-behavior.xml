<?xml version="1.0" encoding="UTF-8"?>
<CodeSystem xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../input-cache/schemas/R4/fhir-single.xsd">
  <id value="snapshot-behavior"/>
  <url value="http://hl7.org/fhir/tools/CodeSystem/snapshot-behavior"/>
  <name value="RenderingEngineViewHintsCS"/>
  <title value="Viewing Hints for Rendering Engines (CodeSystem)"/>
  <status value="active"/>
  <experimental value="false"/>
  <description value="Code that drives Snapshot generation behavior when copying extensions from base to derived"/>
  <jurisdiction>
    <coding>
      <system value="http://unstats.un.org/unsd/methods/m49/m49.htm"/>
      <code value="001"/>
    </coding>
  </jurisdiction>  
  <caseSensitive value="true"/>
  <content value="complete"/>
  <concept>
    <code value="ignore"/>
    <display value="Ignore Extension"/>
    <definition value="Extension does not get copied to derived profile"/>
  </concept>
  <concept>
    <code value="add"/>
    <display value="Add Extension"/>
    <definition value="Extension gets added to whatever already exists"/>
  </concept>
  <concept>
    <code value="defer"/>
    <display value="Defer to Existing Extension"/>
    <definition value="Extension only gets copied if it doesn't already exist"/>
  </concept>
  <concept>
    <code value="overwrite"/>
    <display value="Overwrite Extension"/>
    <definition value="Extension overwrites whatever is already in place"/>
  </concept>
</CodeSystem>