<?xml version="1.0" encoding="UTF-8"?>
<CodeSystem xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../input-cache/schemas/R4/fhir-single.xsd">
  <id value="json-empty-behavior"/>
  <url value="http://hl7.org/fhir/tools/CodeSystem/json-empty-behavior"/>
  <name value="JsonEmptyBehaviorCodes"/>
  <title value="Json Empty Behavior Codes"/>
  <status value="active"/>
  <experimental value="true"/>
  <description value="empty-behaviour: what should be in JSON when a repeating elements has no items. Can be absent, present, or either"/>
  <jurisdiction>
    <coding>
      <system value="http://unstats.un.org/unsd/methods/m49/m49.htm"/>
      <code value="001"/>
    </coding>
  </jurisdiction>  
  <caseSensitive value="true"/>
  <content value="complete"/>
  <concept>
    <code value="absent"/>
    <display value="absent"/>
    <definition value="When there's no items, the json property for the array must be missing"/>
  </concept>
  <concept>
    <code value="present"/>
    <display value="present"/>
    <definition value="When there's no items, the json property for the array must be present and empty"/>
  </concept>
  <concept>
    <code value="either"/>
    <display value="either"/>
    <definition value="When there's no items, the json property for the array may be present and empty, or absent"/>
  </concept>

</CodeSystem>